{
  "name": "@gpt-tag/openai",
  "repository": "github:Create-Inc/gpt-tag",
  "homepage": "https://github.com/Create-Inc/gpt-tag",
  "description": "A library for building OpenAI powered applications using template strings",
  "keywords": [
    "openai",
    "llm",
    "gpt",
    "gpt-3",
    "gpt-4"
  ],
  "version": "0.0.11-alpha.1",
  "license": "MIT",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types.d.ts"
      },
      "require": {
        "types": "./dist/types.d.cts"
      }
    }
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@colors/colors": "^1.6.0",
    "cli-table3": "^0.6.3"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@swc/core": "^1.4.1",
    "@swc/jest": "^0.2.36",
    "@tsconfig/node18": "^18.2.2",
    "@types/libnpmsearch": "^2.0.7",
    "@types/node": "^18.19.15",
    "@types/pacote": "^11.1.8",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "libnpmsearch": "^7.0.1",
    "openai": "^4.67.2",
    "pacote": "^17.0.6",
    "ts-morph": "^21.0.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "tsx": "~4.7.1",
    "typescript": "^5.3.3",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "openai": "^4.67.2"
  }
}
